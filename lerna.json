{
  "version": "2.33.5",
  "npmClient": "bun",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "verifyAccess": false,
      "allowBranch": ["main"]
    },
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): version",
      "allowBranch": ["main"]
    }
  },
  "packages": [
    "apps/*",
    "packages/*"
  ]
}